int main ()
{
    // Declaracao de variaveis


    int numero1;
    int numero2;
    int resultado;

// Entrada 

    printf("Apc - giovanni \n");
    printf ("Soma \n");
    printf ("Informe um numero inteiro:");


    scanf ("%d" , &numero1);
    print("Agora informe o outro numero \n");
    scanf ("%d" , &numero2);

    // Soma dos numeros

    resultado = numero1 + numero2;
    printf ("a soma e igual a %d \n", resultado);

    return 0;






}
